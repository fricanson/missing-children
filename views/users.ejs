<% include header.ejs %>
    <div class="container">
        <h1>Users</h1>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Username</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach((user, index)=> { %>
                    <tr>
                        <td>
                            <%= index + 1 %>
                        </td>
                        <td>
                            <%= user.username %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <% include footer.ejs %>