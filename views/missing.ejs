<% include header.ejs %>

    <% include nav.ejs %>
        <div class="missing-container">
            <h1>Reports</h1>
            <table>
                <thead>
                    <tr>
                        <th>#</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Address</th>
                        <th>Age</th>
                        <th>Phone</th>
                        <th>Location</th>
                        <th>Date</th>
                        <th>Height</th>
                        <th>Complexion</th>
                        <th>Hair</th>
                        <th>Eye Color</th>
                        <th>Clothing</th>
                        <th>Gender</th>
                        <!-- <th>Username</th> -->
                    </tr>
                </thead>
                <tbody>
                    <% persons.forEach((person, index)=> { %>
                        <tr>
                            <td>
                                <%= index + 1 %>
                            </td>
                            <td>
                                <%= person.firstname %>
                            </td>
                            <td>
                                <%= person.lastname %>
                            </td>
                            <td>
                                <%= person.address %>
                            </td>
                            <td>
                                <%= person.age %>
                            </td>
                            <td>
                                <%= person.phone %>
                            </td>
                            <td>
                                <%= person.location %>
                            </td>
                            <td>
                                <%= person.date.toDateString() %>
                            </td>
                            <td>
                                <%= person.height %>
                            </td>
                            <td>
                                <%= person.complexion %>
                            </td>
                            <td>
                                <%= person.hair %>
                            </td>
                            <td>
                                <%= person.color %>
                            </td>
                            <td>
                                <%= person.clothing %>
                            </td>
                            <td>
                                <%= person.gender %>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
        <% include footer.ejs %>